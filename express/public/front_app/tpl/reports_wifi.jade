      component param_search
.row(style='margin-bottom:10px;')
  .col-sm-6
    .col-sm-6
      .form-group.form-inline
        label.small 开始时间:
          .input-group
            span.input-group-addon
              span.glyphicon.glyphicon-calendar
            input.form-control.datetimepicker.valid(name='start_date', maxlength='10', data-date-format='yyyy-mm-dd', style='width:120px' data-autoclose='1', dateiso='true', type='text'  aria-invalid='false')
    .col-sm-6
      .form-group.form-inline
        label.small 结束时间:
          .input-group
            span.input-group-addon
              span.glyphicon.glyphicon-calendar
            input.form-control.datetimepicker.valid(name='end_date', maxlength='10', data-date-format='yyyy-mm-dd', style='width:120px' data-autoclose='1', dateiso='true', type='text'  aria-invalid='false')
