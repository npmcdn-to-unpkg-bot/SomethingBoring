    component select_city
    component select_market
    component select_btn
component table
component paging
button.btn.btn-default.dw_export
  span.glyphicon.glyphicon-export
  | 导出

//-COMPONENT table
table.dw_table.table.table-hover.table-bordered.table-striped
  thead
    tr
      th.text-center 统计日期
      th.text-center 城市
      th.text-center 商场名称
      th.text-center 领券数
      th.text-center 核销数
  tbody
    each val,i in data
      tr
        td=val.statDate
        td=val.cityName
        td=val.marketName
        td=val.getCountNum
        td=val.writeCountNum